<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.moviles">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Moviles"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">

        <activity
            android:name=".LoginActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".PrincipalActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".DatosEmpresaActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".AuditoriasActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".DatosSunatActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ProductosActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CotizacionesActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".InventarioCotizacionesActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VentasActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".InventarioVentasActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".InventarioFacturasActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".IAActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

    </application>

</manifest>
